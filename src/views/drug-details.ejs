<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Drug Details</title>
</head>
<body>
    <h1>Drug Details</h1>
    <% Object.keys(drugDetails).forEach(key => { %>
        <div>
            <h2><%= key %></h2>
            <% if (drugDetails[key].length === 0) { %>
                <p>No items found</p>
            <% } else { %>
                <ul>
                    <% drugDetails[key].forEach(item => { %>
                        <li><%= item.name %>  == <%= item.rxcui %></li>
                    <% }) %>
                </ul>
            <% } %>
        </div>
    <% }) %>
</body>
</html>
